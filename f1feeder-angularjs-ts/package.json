        "css-loader": "^0.28.10",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.11",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.0.6",
        "ng-annotate-loader": "^0.6.1",
        "prettier": "^1.11.1",
        "rimraf": "^2.6.2",
        "style-loader": "^0.20.2",
        "ts-loader": "^3.5.0",
        "tslint": "^5.9.1",
